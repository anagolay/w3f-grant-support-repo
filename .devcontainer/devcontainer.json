{
  "name": "Anagolay -- Project Idiyanale Phase 1 -- W3F delivery",
  "dockerComposeFile": "../devcontainer-dc.yml",
  "service": "code",
  "workspaceFolder": "/workspace",
  "settings": {
    "terminal.integrated.profiles.linux": {
      "zsh": {
        "path": "/usr/bin/zsh"
      }
    },
    "terminal.integrated.defaultProfile.linux": "zsh"
  },
  "extensions": [
    "ms-azuretools.vscode-docker",
    "EditorConfig.EditorConfig",
    "yzhang.markdown-all-in-one",
    "bierner.markdown-mermaid",
    "mhutchie.git-graph",
    "eamodio.gitlens"
  ],
  "overrideCommand": false,
  "features": {
    "docker-in-docker": {
      "version": "latest",
      "moby": true
    },
    "node": {
      "version": "16",
      "nodeGypDependencies": true
    },
    "rust": {
      "version": "latest",
      "profile": "default"
    }
  },
  "remoteUser": "node",
  "postCreateCommand": "bash .devcontainer/install-deps.sh",
  "postStartCommand": "docker-compose up -d",
  "forwardPorts": [
    2112,
    2113,
    9944,
    27017

  ]
}
